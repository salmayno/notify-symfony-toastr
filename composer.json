{
    "name": "php-notify/notify-symfony-toastr",
    "type": "symfony-bundle",
    "description": "Symfony Notify adapter for toastr.js library for php-notify/notify-symfony",
    "keywords": [
        "yoeunes",
        "notify",
        "php",
        "laravel",
        "symfony",
        "Lumen",
        "notifications",
        "messages",
        "alerts",
        "pnotify",
        "toastr ",
        "bundle",
        "flex"
    ],
    "homepage": "https://github.com/php-notify/notify-symfony-toastr",
    "authors": [
        {
            "name": "Younes Khoubza",
            "email": "younes.khoubza@gmail.com",
            "homepage": "https://github.com/yoeunes",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.3",
        "symfony/config": "^2.2|^3.0|^4.0|^5.0",
        "symfony/dependency-injection": "^2.2|^3.0|^4.0|^5.0",
        "symfony/http-kernel": "^2.2|^3.0|^4.0|^5.0",
        "symfony/yaml": "^2.2|^3.0|^4.0|^5.0",
        "php-notify/notify-symfony": "dev-master",
        "php-notify/notify-toastr": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.36"
    },
    "autoload": {
        "psr-4": {
            "Notify\\Symfony\\Toastr\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Notify\\Symfony\\Toastr\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
